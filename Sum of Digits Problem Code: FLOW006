#include<iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int ar[n],res,rem=0;
    for(int i=0;i<n;i++){
        cin>>ar[i];
    }
    for(int i=0;i<n;i++){
        rem=0;
    while(ar[i]!=0){
        res=ar[i]%10;
        rem+=res;
        ar[i]/=10;
    }
    cout<<rem<<endl;
    }
    
    return 0;
}
